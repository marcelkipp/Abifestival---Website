  body
    //- nav hallo
    nav
      .wrapper
        img(src="src/img/logo.png")
        ul
          li Bandinfos
          li Anfahrt
          li Gel&auml;nde
          li Zelten
          li Fragen
          li Sponsoren 
          li Mehr
  
    .video-overlay
    video(autoplay, loop)
      source(src="src/video/background.mp4", type="video/mp4")
    
    .content-container
      .container-video
        div#animate
          img#fadeOutElement(src="src/img/logo.png")
          h1 Abifestival 2016 
          h3 umsonst & und drau&szlig;en 
        .arrow-sontainer
